#
# huawei touchscreen driver configuration
#

config SYNAPTICS_TS
	bool "synaptics touchscreen"
	default n
	help
	  If you say yes here you get support for synaptics touchscreen.
config SYNAPTICS_LCD_TYPE
	bool "synaptics lcd type"
	default n
	help
	  If you say yes here you get support for synaptics touchscreen.
config ATMEL_TS
	bool "atmel touchscreen"
	default n
	help
	  If you say yes here you get support for atmel touchscreen.
menuconfig HW_NOVATEK_TS
	bool "novatek touchscreen"
	default n
	help
	  If you say yes here you get support for novatek touchscreen.
menuconfig ST_TS
	bool "st touchscreen"
	default n
	help
	  If you say yes here you get support for st touchscreen.
