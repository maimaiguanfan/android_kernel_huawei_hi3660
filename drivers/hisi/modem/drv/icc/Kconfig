#
# multicore communication driver
#

comment "multicore communication driver"

config ICC_BALONG
	tristate "support icc"
	depends on IPC_DRIVER
	default n
	help
	  ICC driver

config CA_ICC
	tristate "ca icc"
	default n
	help
	  ca icc

config ICC_BALONG_TEST
	tristate "icc test"
	depends on ICC_BALONG
	default n
	help
	  ICC test